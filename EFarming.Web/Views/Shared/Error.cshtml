@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<hgroup class="title">
    <h1 class="error">Error.</h1>
    <h2 class="error">An error occurred while processing your request.</h2>
</hgroup>

<div class="container">
    <div class="col-md-12">
        <p>Ocurrio un error en el sistema</p>
        <p>Desea enviar y reportar el error</p>
        <hr />
        <textarea class="text-capitalize"></textarea>
        <hr />
    </div>
</div>
