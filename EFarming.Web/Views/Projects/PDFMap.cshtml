@{
    ViewBag.Title = "PDFMap";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Cluster Map</h5>
                </div>
                <div class="ibox-content">
                    <div class="flot-chart farm-search">
                        <div class="row">
                            <div class="col-lg-12">
                                <iframe style="width:1000px;height:765px" src="~/Content/UploadFiles/ClusterEfarming.pdf" typeof="application/pdf"></iframe>
			  <br />
                                <br />
                            </div>
                            <div class="col-lg-6">
                                <a href="~/Content/UploadFiles/ClusterEfarming.pdf" download="ClusterEfarming.pdf" class="btn btn-success btn-sm">Download Map</a>
                            </div>
                            <div class="col-lg-6 text-right">
                                <p>Credits: Carolina Agudelo</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

