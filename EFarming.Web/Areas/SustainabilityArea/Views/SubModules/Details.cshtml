@model EFarming.Core.TasqModule.SubModule

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalle</h2>

<div>
    <h4>Sub Categoría</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Nombre Categoría
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Module.Name)
        </dd>

        <dt>
           Nombre Sub Categoría
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
          Orden
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SubModuleOrder)
        </dd>

        <dt>
        Fecha de creación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CreatedAt)
        </dd>

        <dt>
            Fecha de actualización
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UpdatedAt)
        </dd>

        <dt>
            Fecha de eliminación
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DeletedAt)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Atrás", "Index")
</p>
